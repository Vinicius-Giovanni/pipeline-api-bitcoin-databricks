  "datasets": [
    {
      "name": "393f04d2",
      "displayName": "preco_minimo",
      "queryLines": [
        "SELECT\r\n",
        "  round(valor_brl, 2) AS min_price_brl,\r\n",
        "  timestamps AS min_timestamp\r\n",
        "FROM pipeline_api_bitcoin.bitcoin_data.bitcoin_data\r\n",
        "WHERE criptomoeda = 'BTC'\r\n",
        "ORDER BY valor_brl ASC, timestamps DESC\r\n",
        "LIMIT 1\r\n"
      ]
    },
    {
